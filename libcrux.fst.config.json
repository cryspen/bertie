{
    "fstar_exe": "fstar.exe",
    "options": [
        "--cmi",
        "--warn_error",
        "-331",
        "--cache_checked_modules",
        "--cache_dir",
        "${HAX_HOME}/proof-libs/fstar/.cache",
        "--already_cached",
        "+Prims+FStar+LowStar+C+Spec.Loops+TestLib"
    ],
    "include_dirs": [
        "${HACL_HOME}/lib",
        "${HAX_HOME}/proof-libs/fstar/rust_primitives",
        "${HAX_HOME}/proof-libs/fstar/core",
        "${HAX_HOME}/proof-libs/fstar/hax_lib"
    ]
}
